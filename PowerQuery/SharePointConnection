let
    Source = SharePoint.Contents("https://cpggpc.sharepoint.com/sites/CPCSPCCIAPowerBIDashboard", [ApiVersion = 15]),
    #"Shared Documents" = Source{[Name="Shared Documents"]}[Content],
    General = #"Shared Documents"{[Name="General"]}[Content]
