let
    Source = SharePoint.Contents("https://cpggpc.sharepoint.com/sites/CPCSPCCustomerCareAnalytics", [ApiVersion = 15]),
    #"Shared Documents" = Source{[Name="Shared Documents"]}[Content]
in
    #"Shared Documents"
